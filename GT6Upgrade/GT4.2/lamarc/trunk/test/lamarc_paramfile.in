#parameter file template/sample

#there are two sets of input. One for lam_conv and the other for lamarc

#parameters follow the format, paramater name in caps, followed by an 
#an equal sign, and then the values.
#in the case of multiple values, individual values are separated by space

--LAMCONV_PARAM_BEGIN
#GENETICDATAFILENAME is redundant in the parameter file as it appears in args
#GENETICDATAFILENAME = lamarc_2004_infile.1pop.growth
OUTFILENAME = lamarc_xmldata_infile
GENETICDATATYPE = DNA
GENETICDATAFORMAT = Phylip
GENETICDATAINTERLEAVED = false
REGIONLENGTH = 1000
REGIONOFFSET = 0
LOCUSMAPPOS = 0
MICROREGIONS = 0

--LAMCONV_PARAM_END

--LAMARC_PARAM_BEGIN

FORCES_COALESCENCE_START_VALUE = 0.03 
FORCES_COALESCENCE_METHOD = Watterson 
FORCES_COALESCENCE_MAX_EVENTS = 750 
FORCES_COALESCENCE_PROFILES = fixed 
#---Not needed for sampe data file
#FORCES_GROWTH_START_VALUE = 0.04 
#FORCES_GROWTH_METHOD = FST 
#FORCES_GROWTH_PROFILES = fixed 
#FORCES_GROWTH_MAX_EVENTS = 500 
#FORCES_RECOMBINATION_START_VALUE = 0.06 
#FORCES_RECOMBINATION_METHOD = FST 
#FORCES_RECOMBINATION_PROFILES = percentile 
#FORCES_RECOMBINATION_MAX_EVENTS = 800 
#FORCES_MIGRATION_START_VALUE = 0.03 
#FORCES_MIGRATION_METHOD = Watterson 
#FORCES_MIGRATION_PROFILES = none 
#FORCES_MIGRATION_MAX_EVENTS = 900

CHAINS_REPLICATES = 1 
CHAINS_HEATING_TEMPERATURES = 1.0 
CHAINS_HEATING_SWAP_INTERVAL = 1 
CHAINS_HEATING_ADAPTIVE = true 
CHAINS_STRATEGY_RESIMULATING = 2.9 
CHAINS_INITIAL_NUMBER = 20 
CHAINS_INITIAL_SAMPLES = 550 
CHAINS_INITIAL_DISCARD = 1005 
CHAINS_INITIAL_INTERVAL = 30 
CHAINS_FINAL_NUMBER = 2 
CHAINS_FINAL_SAMPLES = 5000 
CHAINS_FINAL_DISCARD = 1000 
CHAINS_FINAL_INTERVAL = 40 
FORMAT_VERBOSITY = normal 
FORMAT_PROGRESS_REPORTS = verbose 
FORMAT_ECHO = true 
FORMAT_PLOTTING_PROFILE = false 
FORMAT_PLOTTING_POSTERIOR = false 
FORMAT_SEED = 1005 
FORMAT_PARAMETER_FILE = noparamfile.txt 
FORMAT_RESULTS_FILE = lamarc_results.txt 
FORMAT_IN_SUMMARY_FILE = insummary_file.txt 
FORMAT_USE_IN_SUMMARY_FILE = false 
FORMAT_OUT_SUMMARY_FILE = outsummary_file.txt 
