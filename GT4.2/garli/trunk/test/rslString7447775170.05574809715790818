& (rsl_substitution = (CLIENT lysine.umiacs.umd.edu/molehomes/gt4admin/GT6Upgrade/GT4.2/garli/trunk/test/)
                      (SERVER ${GLOBUS_SCRATCH_DIR}/7447775170.05574809715790818/))
  (executable = /fs/mikeproj/sw/RedHat9-32/bin/Garli-2.1_64)
  (directory = $(SERVER))
  (arguments = "garli.conf")
  (stdout = $(SERVER)stdout)
  (stderr = $(SERVER)stderr)
  (jobtype = single)
  (condor_submit = (min_memory 1)                   
                   (architecture INTEL)
                   (operating_system LINUX)
                   (should_transfer_files YES)
                   (when_to_transfer_output ON_EXIT)
                   (transfer_input_files garli.conf,simulated-dna-data.nex)
                   (transfer_output_files garli.boot.tre,garli.screen.log,garli.log00.log)
                   (stream_output False)
                   (stream_error False))
  (file_stage_in = (gsiftp://$(CLIENT)garli.conf file:///$(SERVER)garli.conf)
                   (gsiftp://$(CLIENT)simulated-dna-data.nex file:///$(SERVER)simulated-dna-data.nex))
  (file_stage_out = (file:///$(SERVER)stdout gsiftp://$(CLIENT)stdout)
                    (file:///$(SERVER)stderr gsiftp://$(CLIENT)stderr)
                    (file:///$(SERVER)garli.boot.tre gsiftp://(CLIENT)garli.boot.tre)
                    (file:///$(SERVER)garli.screen.log gsiftp://(CLIENT)garli.screen.log)
                    (file:///$(SERVER)garli.log00.log gsiftp://(CLIENT)garli.log00.log))
  (file_clean_up = file:///$(SERVER))
