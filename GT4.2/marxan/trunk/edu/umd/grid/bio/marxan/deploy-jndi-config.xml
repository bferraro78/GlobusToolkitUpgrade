<?xml version="1.0" encoding="UTF-8"?>
<jndiConfig xmlns="http://wsrf.globus.org/jndi/config">

<!-- Instance service -->
<service name="edu/umd/grid/bio/marxan/MARXANService">
	<resource name="home" type="edu.umd.umiacs.cummings.GSBL.GSBLResourceHome">
	<resourceParams>

		<parameter>
			<name>resourceClass</name>
			<value>edu.umd.umiacs.cummings.GSBL.GSBLResource</value>
		</parameter>

		<parameter>
			<name>factory</name>
			<value>org.globus.wsrf.jndi.BeanFactory</value>
		</parameter>

		<parameter>
			<name>resourceKeyType</name>
			<value>java.lang.Integer</value>
		</parameter>

		<parameter>
			<name>resourceKeyName</name>
			<value>{http://www.umd.edu/namespaces/umiacs/cummings/GSBL}GSBLResourceKey</value>
		</parameter>

	</resourceParams>
	</resource>
</service>

<!-- Factory service -->
<service name="edu/umd/grid/bio/marxan/MARXANFactoryService">
	<resourceLink name="home" target="java:comp/env/services/edu/umd/grid/bio/marxan/MARXANService/home"/>
</service>

</jndiConfig>